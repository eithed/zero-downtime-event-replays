{
    "name": "mannum/zero-downtime-event-replays",
    "description": "Zero downtime event replay for Spatie's Laravel event sourcing package",
    "keywords": [
        "mannum",
        "laravel",
        "zero-downtime-event-replays"
    ],
    "homepage": "https://github.com/mannum/zero-downtime-event-replays",
    "license": "MIT",
    "authors": [
        {
            "name": "Robertbaelde",
            "email": "robert.baelde@gosuperscript.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Mannum\\ZeroDowntimeEventReplays\\": "src",
            "Mannum\\ZeroDowntimeEventReplays\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mannum\\ZeroDowntimeEventReplays\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mannum\\ZeroDowntimeEventReplays\\ZeroDowntimeEventReplaysServiceProvider"
            ],
            "aliases": {
                "ZeroDowntimeEventReplays": "Mannum\\ZeroDowntimeEventReplays\\ZeroDowntimeEventReplaysFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
